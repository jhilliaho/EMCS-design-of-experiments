#!/bin/bash

OUTPUTFILE="test_results/test$1"

> $OUTPUTFILE;

istanbul cover _mocha test/index.test.js --  1-25 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 1-25 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE

istanbul cover _mocha test/index.test.js --  5-16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 5-16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE

istanbul cover _mocha test/index.test.js --  12-16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 12-16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE

istanbul cover _mocha test/index.test.js --  13-15 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 13-15 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE

istanbul cover _mocha test/index.test.js --  7 12 13 15 16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 7 12 13 15 16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE

istanbul cover _mocha test/index.test.js --  7 13 15 16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
istanbul cover _mocha test/index.test.js --  low 7 13 15 16 | egrep -w 'Lines|tests|pass|Running tests' >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
echo " " >> $OUTPUTFILE
